            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion" enabled="true">
              <stringProp name="BeanShellAssertion.query">import java.util.regex.Matcher;
import java.util.regex.Pattern;

String sVerification = vars.get(&quot;pVerifyBreed&quot;);
String strresponse = SampleResult.getResponseDataAsString();
Pattern response_pattern = Pattern.compile(sVerification);
Matcher str_for_match = response_pattern.matcher(strresponse);

ind=0;
Failure = false;

while(str_for_match.find()){
	ind++;
}


if(ind==0){
	Failure = true;
	FailureMessage=&quot;Cannot find breed!&quot;;
}
</stringProp>
